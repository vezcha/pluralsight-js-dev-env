{
  "name": "pluralsight-js-dev-env",
  "version": "1.0.0",
  "description": "Starting kit template",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src//**/*.test.js\"",
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "security-check2": "",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vezcha/pluralsight-js-dev-env.git"
  },
  "author": "vezcha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vezcha/pluralsight-js-dev-env/issues"
  },
  "homepage": "https://github.com/vezcha/pluralsight-js-dev-env#readme",
  "dependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^7.0.6",
    "babel-preset-latest": "^6.24.1",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "css-loader": "^3.1.0",
    "express": "^4.17.1",
    "jsdom": "^7.1.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "open": "^6.4.0",
    "path-dirname": "^1.0.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.36.1",
    "webpack-dev-middleware": "^3.7.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "eslint": "^3.4.0",
    "eslint-watch": "^2.1.14",
    "eslint-plugin-import": "^1.14.0"
  }
}
