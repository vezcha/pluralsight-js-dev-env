{
  "name": "pluralsight-js-dev-env",
  "version": "1.0.0",
  "description": "Starting kit template",
  "main": "index.js",
  "scripts": {
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src//**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node buildScripts/srcServer.js",
    "security-check": "nsp check",
    "security-check2": "",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "share": "npm-run-all --parallel open:src localtunnel",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "npx json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vezcha/pluralsight-js-dev-env.git"
  },
  "author": "vezcha",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vezcha/pluralsight-js-dev-env/issues"
  },
  "homepage": "https://github.com/vezcha/pluralsight-js-dev-env#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "css-loader": "^3.1.0",
    "express": "^4.17.3",
    "jsdom": "^7.1.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "open": "^6.4.0",
    "path-dirname": "^1.0.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.36.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-latest": "^6.16.0",
    "babel-register": "6.16.3",
    "compression": "1.6.2",
    "extract-text-webpack-plugin": "1.0.1",
    "html-webpack-plugin": "2.22.0",
    "json-schema-faker": "0.3.6",
    "eslint": "^3.4.0",
    "eslint-plugin-import": "^1.14.0",
    "eslint-watch": "^2.1.14",
    "rimraf": "2.5.4",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.8.4",
    "webpack-hot-middleware": "2.13.0",
    "webpack-md5-hash": "0.0.5"
  }
}
